  #include<stdio.h>
  int main ()
{
  int n1=20,n2=20,n3=20;
  if(n1>=n2&&n1>=n3);
  printf("%d,is the largest number",n1);
  if(n2>=n1&&n2>=n3);
  printf("%d,is the largest number",n2);
  if(n3>=n1&&n3>=n3);
  printf("%d,is the largest number",n3);
}    







x = 10
while True:
    if x > 5:
        print("x is greater than 5")
        x -= 1  # Decrease x by 1 on each loop
    else:
        print("x is 5 or less")
        break  # Exit the loop when x <= 




#include <stdio.h>

int main() {
    int marks;

    printf("Enter your marks: ");
    scanf("%d", &marks);

    if (marks >= 90) {
        printf("Grade: A\n");
    } 
    else if (marks >= 80) {
        printf("Grade: B\n");
    } 
    else if (marks >= 70) {
        printf("Grade: C\n");
    } 
    else if (marks >= 60) {
        printf("Grade: D\n");
    } 
    else {
        printf("Grade: F\n");
    }

    return 0;
}




    