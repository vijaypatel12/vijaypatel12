
#include <stdio.h>

int main()
{
int a;

int sum=0;
for(a=1; a<=6;a++)
{
    sum=sum+a;}
    printf("%d\n",sum);


    return 0;
}



#include <stdio.h>

int main()
{ int n;
    printf("Enter the value");
    scanf("%d",&n);
    for(int i=1;i<=10; i++){
        printf("%dÃ—%d=%d\n" ,n,i,n*i);
    }

    return 0;
}


 1234 =4321 reverse value

#include <stdio.h>

int main()
{ int n;
    printf("Enter the value");
    scanf("%d",&n);
    int  r=0;
    while(n>0){
    r=r*10;
    r=r+(n%10);
    n=n/10;
    }
    printf("the sum of reverse value %d",r);
    

    
    return 0;
}